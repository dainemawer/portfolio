{
  "name": "dainemawer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "prepare": "husky",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f postgres",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.74.0",
    "@payloadcms/next": "^3.74.0",
    "@payloadcms/plugin-form-builder": "^3.74.0",
    "@payloadcms/plugin-search": "^3.74.0",
    "@payloadcms/plugin-seo": "^3.74.0",
    "@payloadcms/richtext-lexical": "^3.74.0",
    "@payloadcms/storage-vercel-blob": "^3.74.0",
    "graphql": "^16.12.0",
    "i": "^0.3.7",
    "next": "16.1.6",
    "payload": "^3.74.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "sass": "^1.97.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts,json,jsonc,css}": [
      "biome check --write --files-ignore-unknown=true --no-errors-on-unmatched"
    ]
  }
}
